{
  "data": "submissions/incidentes.geojson",
  "templates": [{
    "file": "templates/info-pane.tpl"
  }],
  "options": {
    "timestamp": "today",
    "image": "foto",
    "color": "tipo"
  },
  "filters": [{
    "type": "continuous",
    "field": "today",
    "expanded": true
  }, {
